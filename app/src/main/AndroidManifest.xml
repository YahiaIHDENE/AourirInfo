<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.glog.aourir_infos">

    <application
        android:allowBackup="true"
        android:icon="@drawable/aourirlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sign_up"></activity>
        <activity android:name=".fgtPassword" />
        <activity android:name=".Login" />
        <activity android:name=".LuncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RdvActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NewRdvActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MessageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="Aourir infos"
            android:theme="@style/AppTheme.NoActionBar" />



        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCVwIFVJ-H-l6BS8iPZyrfDBdOne0UxO4o"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <service
            android:name=".notifications.MyFirebaseMessaging"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name=".notifications.MyfirebaseIdService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_SERVICE" />
            </intent-filter>
        </service>
    </application>

</manifest>